{
  "name": "bwell",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "restart": "expo start -c",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "type-check": "tsc --noEmit --incremental",
    "lint:eslint": "eslint \"**/*.*{ts,tsx,js,jsx}\"",
    "lint:prettier": "prettier --check .",
    "lint": "expo lint && npm run lint:eslint  && npm run lint:prettier && npm run type-check",
    "prepare": "husky",
    "clean": "rm -rf node_modules .expo .expo-shared && npm ci && npx expo install --fix",
    "start-clean": "expo start -c",
    "verify": "npx expo-doctor",
    "fix": "npx expo install --check",
    "upgrade": "expo install --fix && expo install --check && npx expo-doctor",
    "publish": "eas update --branch main --environment production --message \"Update $(date)\"",
    "publish-web": "npx expo export --platform web && eas deploy --environment production",
    "update-types": "npm uninstall @milobedini/shared-types && npm i @milobedini/shared-types -D"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.1.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/material-top-tabs": "^7.3.6",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/react-query": "^5.82.0",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "expo": "^54.0.10",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-mail-composer": "~15.0.7",
    "expo-router": "~6.0.8",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.11",
    "expo-video": "~3.0.11",
    "expo-web-browser": "~15.0.7",
    "formik": "^2.4.6",
    "lottie-react-native": "~7.3.1",
    "moti": "^0.30.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-confirmation-code-field": "^8.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dates": "^0.22.49",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "toastify-react-native": "^7.2.3",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
